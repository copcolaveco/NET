    Private Sub listarPorFecha2()
        Dim x1app As Microsoft.Office.Interop.Excel.Application
        Dim x1libro As Microsoft.Office.Interop.Excel.Workbook
        Dim x1hoja As Microsoft.Office.Interop.Excel.Worksheet
        x1app = CType(CreateObject("Excel.Application"), Microsoft.Office.Interop.Excel.Application)
        x1libro = CType(x1app.Workbooks.Add, Microsoft.Office.Interop.Excel.Workbook)
        x1hoja = CType(x1libro.Worksheets(1), Microsoft.Office.Interop.Excel.Worksheet)

        Dim a As New dATB
        Dim ae As New dATB
        Dim listafichas As New ArrayList
        Dim listamuestras As New ArrayList
        Dim lista As New ArrayList
        Dim fila As Integer = 1
        Dim columna As Integer = 1
        Dim desde As Date = DtpDesde2020.Value.ToString("yyyy-MM-dd")
        Dim hasta As Date = DtpHasta2020.Value.ToString("yyyy-MM-dd")
        Dim fecdesde As String
        Dim fechasta As String
        'Microorganismos------------------------------------------
        Dim sincrecimiento As Integer = 0
        Dim contaminada As Integer = 0
        Dim estafilococoaureus As Integer = 0
        Dim estreptococodysgalactiae As Integer = 0
        Dim estreptococouberis As Integer = 0
        Dim estreptococoagalactiae As Integer = 0
        Dim corynebacteriumbovis As Integer = 0
        Dim estafilocococoagnegativo As Integer = 0
        Dim ecoli As Integer = 0
        Dim levadura As Integer = 0
        Dim protothecaspp As Integer = 0
        Dim bacilogrampositivo As Integer = 0
        Dim klebsiellaspp As Integer = 0
        Dim proteusspp As Integer = 0
        Dim arcanobacteriumpyogenes As Integer = 0
        Dim pseudomonas As Integer = 0
        Dim pseudomonasspp As Integer = 0
        Dim serratiaspp As Integer = 0
        'Dim bacillusspp As Integer = 0
        'Dim micrococo As Integer = 0
        '---------------------------------------------------------
        'Resistencia----------------------------------------------


        '---------------------------------------------------------
        Dim contador As Integer = 0
        Dim unagente As Integer = 0
        Dim dosagentes As Integer = 0
        Dim tresagentes As Integer = 0
        Dim masagentes As Integer = 0

        fecdesde = Format(desde, "yyyy-MM-dd")
        fechasta = Format(hasta, "yyyy-MM-dd")

        Dim fichas As Integer = 0
        Dim muestras As Integer = 0
        Dim idATB As Long = 0

        listafichas = a.listarxfichaDesdeHasta(fecdesde, fechasta)
        If Not listafichas Is Nothing Then
            If listafichas.Count > 0 Then
                For Each a In listafichas
                    idATB = a.FICHA
                    listamuestras = ae.listar_muestras(idATB)
                    muestras = muestras + listamuestras.Count
                    Dim atb As New dATB
                    Dim listaatb As New ArrayList
                    Dim idsol As Long = a.FICHA
                    listaatb = atb.listar_muestras(idsol)
                    If Not listaatb Is Nothing Then
                        If listaatb.Count > 0 Then
                            For Each atb In listaatb
                                Dim agentes As New dATB
                                Dim listaagentes As New ArrayList
                                Dim idanimal As String = atb.MUESTRA
                                listaagentes = agentes.listarxfichaxmuestra(idsol, idanimal)
                                If Not listaagentes Is Nothing Then
                                    If listaagentes.Count > 0 Then
                                        If listaagentes.Count = 1 Then
                                            unagente = unagente + 1
                                        ElseIf listaagentes.Count = 2 Then
                                            dosagentes = dosagentes + 1
                                        ElseIf listaagentes.Count = 3 Then
                                            tresagentes = tresagentes + 1
                                        ElseIf listaagentes.Count > 3 Then
                                            masagentes = masagentes + 1
                                        End If
                                    End If
                                End If
                                agentes = Nothing
                                listaagentes = Nothing

                                idanimal = 0
                            Next
                        End If
                    End If
                    atb = Nothing
                    listaatb = Nothing
                    idsol = 0
                Next
            End If
        End If

        ''*** LISTAR RESISTENCIA ANTIBIOTICOS *******************************************************
        Dim _atb As New dATB
        Dim _lista As New ArrayList
        Dim matriz(21, 11, 3) As Integer

        Dim i As Integer = 0
        Dim j As Integer = 0
        Dim k As Integer = 0
        For i = 0 To 20
            For j = 0 To 10
                For k = 0 To 2
                    matriz(i, j, k) = 0
                    k = k + 1
                Next
                j = j + 1
            Next
            i = i + 1
        Next

        _lista = _atb.listarporfecha(fecdesde, fechasta)
        If Not _lista Is Nothing Then
            If _lista.Count > 0 Then
                For Each _atb In _lista
                If _atb.AISLAMIENTO = 0 Then
                    If _atb.ATB = 1 Then
                        If _atb.RESISTENCIA = "-" Then
                            matriz(0, 1, 0) = matriz(0, 1, 0) + 1
                        ElseIf _atb.RESISTENCIA = "R" Then
                            matriz(0, 1, 1) = matriz(0, 1, 1) + 1
                        ElseIf _atb.RESISTENCIA = "S" Then
                            matriz(0, 1, 2) = matriz(0, 1, 2) + 1
                        End If
                    ElseIf _atb.ATB = 2 Then
                            If _atb.RESISTENCIA = "-" Then
                                matriz(0, 2, 0) = matriz(0, 2, 0) + 1
                            ElseIf _atb.RESISTENCIA = "R" Then
                                matriz(0, 2, 1) = matriz(0, 2, 1) + 1
                            ElseIf _atb.RESISTENCIA = "S" Then
                                matriz(0, 2, 2) = matriz(0, 2, 2) + 1
                            End If
                    ElseIf _atb.ATB = 3 Then
                        If _atb.RESISTENCIA = "-" Then
                                matriz(0, 3, 0) = matriz(0, 3, 0) + 1
                        ElseIf _atb.RESISTENCIA = "R" Then
                                matriz(0, 3, 1) = matriz(0, 3, 1) + 1
                        ElseIf _atb.RESISTENCIA = "S" Then
                                matriz(0, 3, 2) = matriz(0, 3, 2) + 1
                        End If
                        ElseIf _atb.ATB = 4 Then
                            If _atb.RESISTENCIA = "-" Then
                                matriz(0, 4, 0) = matriz(0, 4, 0) + 1
                            ElseIf _atb.RESISTENCIA = "R" Then
                                matriz(0, 4, 1) = matriz(0, 4, 1) + 1
                            ElseIf _atb.RESISTENCIA = "S" Then
                                matriz(0, 4, 2) = matriz(0, 4, 2) + 1
                            End If
                     ElseIf _atb.ATB = 5 Then
                        If _atb.RESISTENCIA = "-" Then
                                matriz(0, 5, 0) = matriz(0, 5, 0) + 1
                        ElseIf _atb.RESISTENCIA = "R" Then
                                matriz(0, 5, 1) = matriz(0, 5, 1) + 1
                        ElseIf _atb.RESISTENCIA = "S" Then
                                matriz(0, 5, 2) = matriz(0, 5, 2) + 1
                        End If
                        ElseIf _atb.ATB = 6 Then
                            If _atb.RESISTENCIA = "-" Then
                                matriz(0, 6, 0) = matriz(0, 6, 0) + 1
                            ElseIf _atb.RESISTENCIA = "R" Then
                                matriz(0, 6, 1) = matriz(0, 6, 1) + 1
                            ElseIf _atb.RESISTENCIA = "S" Then
                                matriz(0, 6, 2) = matriz(0, 6, 2) + 1
                            End If
                    ElseIf _atb.ATB = 7 Then
                        If _atb.RESISTENCIA = "-" Then
                                matriz(0, 7, 0) = matriz(0, 7, 0) + 1
                        ElseIf _atb.RESISTENCIA = "R" Then
                                matriz(0, 7, 1) = matriz(0, 7, 1) + 1
                        ElseIf _atb.RESISTENCIA = "S" Then
